# LLM API Keys
# Cerebras (Primary) - Add as many as needed for failover
CEREBRAS_API_KEY=your-cerebras-key
# CEREBRAS_API_KEY_1=
# CEREBRAS_API_KEY_2=

# OpenRouter (Fallback) - Add as many as needed for failover
OPENROUTER_KEY_1=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENROUTER_KEY_2=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Auth & Database (Primary) - Nhost (PostgreSQL + Hasura)
NEXT_PUBLIC_NHOST_SUBDOMAIN=your-subdomain
NEXT_PUBLIC_NHOST_REGION=your-region

# Alternative/Legacy Database Configurations
# Turso (LibSQL)
TURSO_DATABASE_URL=libsql://your-db.turso.io
TURSO_AUTH_TOKEN=your-turso-token

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics
# NEXT_PUBLIC_PLAUSIBLE_DOMAIN=your-domain.com
